<OMBIND><OMS cd="quant1" name="forall"/><OMBVAR><OMATTR><OMATP><OMS cd="mltt" name="type"/><OMA brackets-hint="on"><OMS cd="mltt" name="funtype"/><OMV name="N"/><OMS cd="set.theory" name="first-n-nats"/></OMA></OMATP><OMV name="I"/></OMATTR></OMBVAR><OMA brackets-hint="on"><OMS cd="relation1" name="eq"/>
<OMA brackets-hint="on"><OMV name="X"/><OMS cd="mltt" name="ind"/></OMA>
